        const HOUSE_VALUES = {
            straw: 1, wood: 3, brick: 5, gold: 25, diamond: 100
        };

        // ==========================================
        // PROVABLY FAIR SYSTEM
        // ==========================================
        
        class ProvablyFair {
            constructor() {
                this.serverSeed = this.generateSeed();
                this.clientSeed = 'client_' + Math.random().toString(36).substr(2, 9);
                this.nonce = 0;
                this.hash = this.hashSeed(this.serverSeed + this.clientSeed);
            }
            
            generateSeed() {
                return 'srv_' + Math.random().toString(36).substr(2, 16) + Date.now().toString(36);
            }
            
            hashSeed(seed) {
                let hash = 0;
                for (let i = 0; i < seed.length; i++) {
                    const char = seed.charCodeAt(i);
                    hash = ((hash << 5) - hash) + char;
                    hash = hash & hash;
                }
                return Math.abs(hash).toString(16);
            }
            
            getNextFloat() {
                const combined = this.serverSeed + this.clientSeed + this.nonce++;
                const hash = this.hashSeed(combined);
                const int = parseInt(hash.substr(0, 8), 16);
                return int / 0xFFFFFFFF;
            }
            
            verify(serverSeed, clientSeed, nonce, result) {
                const combined = serverSeed + clientSeed + nonce;
                const hash = this.hashSeed(combined);
                const int = parseInt(hash.substr(0, 8), 16);
                const expected = int / 0xFFFFFFFF;
                return Math.abs(expected - result) < 0.0001;
            }
        }

        const pf = new ProvablyFair();

        // ==========================================
        // REALISTIC PROBABILITY ENGINE
        // ==========================================
        
        const PROBABILITIES = {
            // Crash: 1% house edge, uses formula: 0.99 / (1 - random)
            crash: {
                getMultiplier: () => {
                    const r = pf.getNextFloat();
                    if (r < 0.06) return 1.00; // 6% instant crash
                    const houseEdge = 0.99;
                    const multiplier = Math.max(1.00, houseEdge / (1 - r));
                    return Math.floor(multiplier * 100) / 100;
                }
            },
            
            // Dice: Standard 49.5% win rate on 2x (1% house edge)
            dice: {
                roll: () => {
                    return Math.floor(pf.getNextFloat() * 10000) / 100;
                },
                winChance: (target, isOver) => {
                    if (isOver) return (100 - target) * 0.99;
                    return target * 0.99;
                }
            },
            
            // Mines: Binomial distribution
            mines: {
                generateGrid: (minesCount) => {
                    const grid = Array(25).fill(false);
                    let placed = 0;
                    while (placed < minesCount) {
                        const pos = Math.floor(pf.getNextFloat() * 25);
                        if (!grid[pos]) {
                            grid[pos] = true;
                            placed++;
                        }
                    }
                    return grid;
                },
                getMultiplier: (revealed, minesCount) => {
                    const total = 25;
                    const safe = total - minesCount;
                    let prob = 1;
                    for (let i = 0; i < revealed; i++) {
                        prob *= (safe - i) / (total - i);
                    }
                    return Math.max(1, (0.97 / prob));
                }
            },
            
            // Plinko: Binomial distribution based on rows - HARDER TO WIN
            plinko: {
                drop: (rows) => {
                    let position = 0;
                    for (let i = 0; i < rows; i++) {
                        if (pf.getNextFloat() > 0.5) position++;
                    }
                    return position;
                },
                multipliers: {
                    low: {
                        8: [5.6, 2.1, 1.1, 1.0, 0.5, 1.0, 1.1, 2.1, 5.6],
                        12: [10, 3, 1.6, 1.4, 1.1, 1.0, 0.5, 1.0, 1.1, 1.4, 1.6, 3, 10],
