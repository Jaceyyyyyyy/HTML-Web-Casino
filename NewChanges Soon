            },
            
            // Wheel: Configurable segments
            wheel: {
                spin: (segments) => {
                    return Math.floor(pf.getNextFloat() * segments);
                },
                standard: {
                    segments: [1, 1, 1, 1, 1, 2, 2, 2, 5, 10],
                    colors: ['#ef4444', '#3b82f6', '#22c55e', '#f59e0b', '#8b5cf6', '#06b6d4', '#f97316', '#ec4899', '#10b981', '#fbbf24']
                }
            },
            
            // Tower: 50/50 per level with increasing multipliers
            tower: {
                checkSafe: () => pf.getNextFloat() > 0.5,
                multipliers: [1.5, 2.25, 3.375, 5.06, 7.59, 11.39, 17.08, 25.62, 38.44, 57.67]
            },
            
            // Hi-Lo: Card probability based on remaining deck
            hilo: {
                draw: () => Math.floor(pf.getNextFloat() * 13) + 1, // 1-13 (A-K)
                probability: (current, guess) => {
                    if (guess === 'high') return (13 - current) / 12;
                    return (current - 1) / 12;
                }
            },
            
            // Pump: Increasing risk curve
            pump: {
                checkBurst: (level) => {
                    const baseChance = 0.1;
                    const risk = baseChance + (level * 0.05);
                    return pf.getNextFloat() < risk;
                },
                multipliers: [1.2, 1.5, 2, 2.8, 4, 6, 9, 14, 22, 35]
            },
            
            // Slide: Precision timing
            slide: {
                getTarget: () => 15 + Math.floor(pf.getNextFloat() * 70), // 15-85%
                getSpeed: () => 0.5 + pf.getNextFloat() * 1.5 // Variable speed
            },
            
            // Diamonds: 1/5 chance per tile
            diamonds: {
                checkWin: () => pf.getNextFloat() < 0.20,
                multipliers: [1.25, 1.5, 2, 3, 5, 10]
            },
            

