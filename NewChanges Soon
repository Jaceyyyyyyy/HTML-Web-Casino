                    },
                    medium: {
                        8: [13, 3, 1.3, 0.7, 0.4, 0.7, 1.3, 3, 13],
                        12: [24, 6, 3, 1.8, 1.2, 0.6, 0.4, 0.6, 1.2, 1.8, 3, 6, 24],
                        16: [41, 10, 5, 3, 1.9, 1.2, 0.6, 0.4, 0.2, 0.4, 0.6, 1.2, 1.9, 3, 5, 10, 41]
                    },
                    high: {
                        8: [29, 4, 1.5, 0.3, 0.2, 0.3, 1.5, 4, 29],
                        12: [58, 12, 5, 2.1, 1.1, 0.5, 0.2, 0.5, 1.1, 2.1, 5, 12, 58],
                        16: [120, 25, 10, 5, 2.8, 1.4, 0.6, 0.2, 0.2, 0.2, 0.6, 1.4, 2.8, 5, 10, 25, 120]
                    }
